{
    "builds":[{
        "src": "full_auth/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        }],
        "routes":[
            {
                "src": "/(.*)",
                "dest": "full_auth/wsgi.py"
            }
        ]
}